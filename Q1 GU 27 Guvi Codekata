import java.util.Scanner;

// Generic Pair class
class Pair<T, U> {
    private T first;
    private U second;

    // Constructor
    public Pair(T first, U second) {
        this.first = first;
        this.second = second;
    }

    // Method to compare two pairs
    public boolean equals(Pair<T, U> otherPair) {
        return this.first.equals(otherPair.first) && this.second.equals(otherPair.second);
    }

    // Print comparison result
    public void printComparisonResult(Pair<T, U> otherPair) {
        if (this.equals(otherPair)) {
            System.out.println("The pairs are equal.");
        } else {
            System.out.println("The pairs are not equal.");
        }
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Input for the first pair
        
        Integer firstInt1 = Integer.parseInt(scanner.nextLine());
        
        String firstStr1 = scanner.nextLine();

        // Input for the second pair
        
        Integer firstInt2 = Integer.parseInt(scanner.nextLine());
       
        String firstStr2 = scanner.nextLine();

        // Create two pairs
        Pair<Integer, String> pair1 = new Pair<>(firstInt1, firstStr1);
        Pair<Integer, String> pair2 = new Pair<>(firstInt2, firstStr2);

        // Compare the pairs and print the result
        pair1.printComparisonResult(pair2);

        scanner.close();
    }
}
